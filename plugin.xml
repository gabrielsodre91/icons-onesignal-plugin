<?xml version="1.0" encoding="UTF-8"?>  
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" id="icons-onesignal-plugin" version="1.0.0"> 

    <name>Define icons to OneSignal push notification</name> 
    <description>Add customized icon to the OneSignal push notifications service by moving the image files to the requiered folders by OneSignal SDK</description> 
    <license>MIT</license> 

    <engines> 
        <engine name="cordova" version=">=3.0.0" /> 
    </engines>  

    <!-- android --> 
    <platform name="android">
        
        <resource-file src="src/ic_stat_onesignal_default_mdpi.png" target="res/drawable-mdpi/ic_stat_onesignal_default.png" />	
		<resource-file src="src/ic_stat_onesignal_default_hdpi.png" target="res/drawable-hdpi/ic_stat_onesignal_default.png" />
		<resource-file src="src/ic_stat_onesignal_default_xhdpi.png" target="res/drawable-xhdpi/ic_stat_onesignal_default.png" />
		<resource-file src="src/ic_stat_onesignal_default_xxhdpi.png" target="res/drawable-xxhdpi/ic_stat_onesignal_default.png" />
                
    </platform>  
    
</plugin>